{
  "name": "@human-os/analysis",
  "version": "0.1.0",
  "description": "Unified semantic analysis for interviews, journals, and transcripts - emotion scoring, competency assessment, and archetype classification",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./scoring": {
      "types": "./dist/scoring/index.d.ts",
      "import": "./dist/scoring/index.js"
    },
    "./lexicons": {
      "types": "./dist/lexicons/index.d.ts",
      "import": "./dist/lexicons/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@human-os/core": "workspace:*",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^2.1.0"
  }
}
