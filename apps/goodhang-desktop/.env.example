# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# API Configuration
VITE_API_BASE_URL=https://api.goodhang.com

# Feature Flags
# Enable the new v0-style workflow layout (wider sidebar with chat)
VITE_USE_WORKFLOW_MODE_LAYOUT=false
# Enable workflow state persistence (localStorage + Supabase sync)
VITE_ENABLE_WORKFLOW_PERSISTENCE=false
# Enable step actions (snooze, skip) in workflow progress
VITE_ENABLE_STEP_ACTIONS=true
